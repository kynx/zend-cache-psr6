{
    "name": "kynx/zend-cache-psr6",
    "description": "PSR-6 adapter for Zend Cache",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Matt Kynaston",
            "email": "matt@claritum.com"
        }
    ],
    "autoload" : {
        "psr-4" : {
            "Kynx\\ZendCache\\Psr\\" : "src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "KynxTest\\ZendCache\\Psr\\" : "test/"
        }
    },
    "require": {
        "php" : ">=5.5",
        "psr/cache": "^1.0",
        "zendframework/zend-cache": "dev-develop",
        "zendframework/zend-servicemanager": "dev-develop as 2.7"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "squizlabs/php_codesniffer": "^2.3.1"
    }
}
